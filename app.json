{
  "expo": {
    "name": "KukuFarm",
    "slug": "KukuFarm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app uses the photo library to upload images.",
          "cameraPermission": "This app uses the camera to take photos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for delivery purposes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b7d96688-d94a-4e2e-a10d-db771b358335"
      }
    },
    "android": {
      "package": "com.costantine.KukuFarm"
    }
  }
}
