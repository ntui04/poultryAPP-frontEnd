{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router",
      "expo-image-picker", 
      [
        "expo-image-picker",
        {
          "photosPermission": "This app uses the photo library to upload images.",
          "cameraPermission": "This app uses the camera to take photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
